{
    "lastTimeStamp": "",
    "file_name_template": "{number}.md",
    "output_path": "issues",
    "max_issues_per_run": 50,
    "source_repository": "https://github.com/shridhar-tl/jira-assistant",
    "autoCommit": true,
    "md_config": {
        "heading_start": 2,
        "include_images": false,
        "comment_title_format": null,
        "dateFormat": "MMM dd, yyyy",
        "header": {
            "slug": "{number}/{title|cut:50}",
            "date": "{updated_at??created_at}",
            "title": "{title}"
        }
    }
}
