{
    "lastTimeStamp": "2024-08-13T14:54:51.000Z",
    "file_name_template": "{number}.md",
    "output_path": "issues",
    "max_issues_per_run": 55,
    "source_repository": "https://github.com/shridhar-tl/jira-assistant",
    "autoCommit": true,
    "md_config": {
        "heading_start": 2,
        "include_images": false,
        "comment_title_format": null,
        "dateFormat": "MMM dd, yyyy",
        "header": {
            "id": "{number}",
            "slug": "{title|cut:50}",
            "date": "{updated_at|yyyy-MM-dd??created_at|yyyy-MM-dd}",
            "title": "{title}"
        }
    }
}
